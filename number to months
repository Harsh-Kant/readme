#include <iostream>

using namespace std;

int main()
{
    int num;
    cout<<"Enter the number"<<endl;
    cin>>num;
    switch(num)
    {
        case 1:cout<<endl<<"January"<<endl;
        break;
        case 2:cout<<endl<<"February"<<endl;
        break;
        case 3:cout<<endl<<"March"<<endl;
        break;
        case 4:cout<<endl<<"April"<<endl;
        break;
        case 5:cout<<endl<<"May"<<endl;
        break;
        case 6:cout<<endl<<"June"<<endl;
        break;
        case 7:cout<<endl<<"July"<<endl;
        break;
        case 8:cout<<endl<<"August"<<endl;
        break;
        case 9:cout<<endl<<"September"<<endl;
        break;
        case 10:cout<<endl<<"October"<<endl;
        break;
        case 11:cout<<endl<<"November"<<endl;
        break;
        case 12:cout<<endl<<"December"<<endl;
        break;
        default :cout<<endl<<"Error!Invalid input"<<endl;
        break;
    }
    return 0;
}
