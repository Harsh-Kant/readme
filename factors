#include <iostream>

using namespace std;

int main()
{
    int num,i,rem;
    cout<<"Enter a number ";
    cin>>num;
    if(num<0)
    {
        num=(-1)*num;
    }
    if(num==0)
    {
        cout<<"NO factors"<<endl;
    }
    else
    {
        cout<<"Factors are ";
        for(i=1;i<=num;i++)
    {
        rem=num%i;
        if(rem==0)
        {
            cout<<i<<" ";
        }

    }


    }
    return 0;
}
