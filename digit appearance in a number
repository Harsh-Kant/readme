#include <iostream>

using namespace std;

int main()
{
    int num,key,rem,count=0,no;
    cout<<"Enter the number ";
    cin>>num;
    no=num;
    cout<<"Enter the key ";
    cin>>key;
    while(num!=0)
    {
        rem=num%10;
        if(key==rem)
        {
            count++;
        }
        num=num/10;
    }
    cout<<endl<<key<<" appears "<<count<<" times in "<<no<<endl;
    return 0;
}
